{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFproj1new"
		},
		"AzureSqlDatawarehouse_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatawarehouse'"
		},
		"Datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Datalake'"
		},
		"AzureSqlDatawarehouse_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "vkserveradf1.database.windows.net"
		},
		"AzureSqlDatawarehouse_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DataWarehouse"
		},
		"AzureSqlDatawarehouse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"Datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountadfpro1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFproj1new_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatawarehouse_password": {
						"value": "[parameters('AzureSqlDatawarehouse_password')]"
					},
					"Datalake_accountKey": {
						"value": "[parameters('Datalake_accountKey')]"
					},
					"AzureSqlDatawarehouse_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatawarehouse_properties_typeProperties_server')]"
					},
					"AzureSqlDatawarehouse_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatawarehouse_properties_typeProperties_database')]"
					},
					"AzureSqlDatawarehouse_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatawarehouse_properties_typeProperties_userName')]"
					},
					"Datalake_properties_typeProperties_url": {
						"value": "[parameters('Datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}